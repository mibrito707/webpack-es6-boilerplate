{
    "name": "webpack-es6-boilerplate",
    "version": "1.0.1",
    "author": "Maurys Brito <mibrito707@gmail.com> (https://mibrito.com)",
    "description": "A delightfully Webpack ES6 Boilerplate",
    "license": "MIT",
    "keywords": [
        "webpack",
        "boilerplate",
        "es6",
        "babel",
        "eslint",
        "tape",
        "sass"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/mibrito707/webpack-es6-boilerplate"
    },
    "main": "src/index.js",
    "scripts": {
        "start": "webpack-dev-server --config conf/webpack.dev.js",
        "watch": "webpack --env=dev --watch --progress --profile --colors",
        "build": "webpack --env=prod --progress --profile --colors",
        "lint": "eslint ./src/**.js",
        "test": "tape -r babel-register tests/*.js || true"
    },
    "devDependencies": {
        "babel-core": "6.26.0",
        "babel-eslint": "7.2.3",
        "babel-jest": "20.0.3",
        "babel-loader": "7.1.2",
        "babel-preset-env": "1.6.0",
        "clean-webpack-plugin": "0.1.16",
        "css-loader": "^0.28.5",
        "eslint": "4.5.0",
        "eslint-loader": "1.9.0",
        "extract-text-webpack-plugin": "3.0.0",
        "html-webpack-plugin": "2.30.1",
        "tape": "",
        "node-sass": "4.5.3",
        "sass-loader": "6.0.6",
        "style-loader": "^0.18.2",
        "webpack": "3.5.5",
        "webpack-dev-server": "2.7.1",
        "webpack-merge": "4.1.0"
    },
    "dependencies": {}
}